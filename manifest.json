{
  "manifest_version": 3,
  "name": "Simple Vim Scrolling",
  "version": "1.0",
  "description": "Navigate pages using vim-style keyboard shortcuts",
  "icons": {
    "16": "icons/16x16.png",
    "32": "icons/32x32.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "content_scripts": [

    {
      "matches": ["<all_urls>"],
      "js": ["page-navigator.js"]
    }
  ],
  "action": {
    "default_icon": {
	"16": "icons/16x16.png",
	"32": "icons/32x32.png",
	"48": "icons/48x48.png",
	"128": "icons/128x128.png"
    }

  },

  "permissions": []
}
